    <element name="LugeonTest" substitutionGroup="diggs:AbstractInsituTestProcedure"
        type="diggs:LugeonTestType"/>
    <complexType mixed="false" name="LugeonTestType">
        <complexContent>
            <extension base="diggs:AbstractInsituTestProcedureType">
                <sequence>
                    <element minOccurs="0" name="depthToGroundWater" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Distance from ground surface to the top of the saturated
                                zone (water table)</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="pressureMeasurementConfiguration">
                        <annotation>
                            <documentation>Describes how pressure is measured during the test; from
                                a controlled list. Allowed values are: - pressure gauge - borehole
                                transducer</documentation>
                        </annotation>
                        <simpleType>
                            <restriction base="string">
                                <enumeration value="pressure gauge"/>
                                <enumeration value="borehole transducer"/>
                            </restriction>
                        </simpleType>
                    </element>
                    <element minOccurs="0" name="pressureGaugeHeight" type="eml:LengthMeasure">
                        <annotation>
                            <documentation>Height above ground surface to the pressure gauge
                                location, if pressure gauge is used instead of a borehole
                                transducer</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="pMax" type="eml:PressureMeasure">
                        <annotation>
                            <documentation>The maximum test pressure, defined so that it does not
                                exceed the in-situ minimum stress, thus avoiding hydraulic
                                fracturing.</documentation>
                        </annotation>
                    </element>
                    <element maxOccurs="unbounded" minOccurs="0" name="lugeonStep"
                        type="diggs:LugeonStepPropertyType">
                        <annotation>
                            <documentation>Contains properties for each step of the lugeeon test run
                                at various injection pressures. There are typically five steps in a
                                lugeon test. </documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="LugeonStepPropertyType">
        <sequence>
            <element maxOccurs="1" minOccurs="1" ref="diggs:LugeonStep"/>
        </sequence>
    </complexType>
    <element abstract="false" name="LugeonStepTimeSeries" substitutionGroup="gml:AbstractCoverage"
        type="diggs:LugeonStepTimeSeriesType">
        <annotation>
            <documentation>An object containing a Lugeon test step time series.</documentation>
        </annotation>
    </element>
    <element name="LugeonStep" substitutionGroup="diggs:AbstractObject" type="diggs:LugeonStepType">
        <annotation>
            <documentation>An object that carries properties for a Lugeon test step</documentation>
        </annotation>
    </element>
    <complexType name="LugeonStepType">
        <complexContent>
            <extension base="diggs:AbstractObjectType">
                <sequence>
                    <element minOccurs="1" name="lugeonStepNumber" type="positiveInteger">
                        <annotation>
                            <documentation>The number of this step, with 1 representing the first
                                step, 2 the second step, etc.</documentation>
                        </annotation>
                    </element>
                    <element name="pressure" type="eml:PressureMeasure" minOccurs="0">
                        <annotation>
                            <documentation>The target pressure for the step</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="avgFlowRate" type="eml:VolumePerTimeMeasure">
                        <annotation>
                            <documentation>The average flow rate for the step, derived from the flow
                                measurements recorded in the step time series. </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="coeffPermeability" type="eml:LengthPerTimeMeasure">
                        <annotation>
                            <documentation>The coefficient of permeability (also known as hydraulic
                                conductivity) computed from the step measurements. This value is
                                averaged with other step results and is reported as one of the
                                primary test results </documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="lugeonValue" type="double">
                        <annotation>
                            <documentation>The Lugeon value, a measure of hydraulic conductivity in
                                rock, as computed from the step measurements. This value is averaged
                                with other step results and is reported as one of the primary test
                                results </documentation>
                        </annotation>
                    </element>
                    <element name="lugeonStepTimeSeries"
                        type="diggs:LugeonStepTimeSeriesPropertyType" minOccurs="0" maxOccurs="1">
                        <annotation>
                            <documentation>Property that contains the recorded flow for the test
                                step.</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="totalElapsedTime" type="eml:TimeMeasure">
                        <annotation>
                            <documentation>The total elapsed time for the step; this is typically 10
                                minutes, but could be different if steady-state flow
                                occurs</documentation>
                        </annotation>
                    </element>
                    <element minOccurs="0" name="totalVolumeWater" type="eml:VolumeMeasure">
                        <annotation>
                            <documentation>The total volume of water pumped into the borehole during
                                this step</documentation>
                        </annotation>
                    </element>
                </sequence>
            </extension>
        </complexContent>
    </complexType>
    <complexType name="LugeonStepTimeSeriesPropertyType">
