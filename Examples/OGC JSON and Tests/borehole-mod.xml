              <minusUncertaintyValue>5</minusUncertaintyValue>
              <confidenceLevel uom="%">68</confidenceLevel>
            </Uncertainty>
          </linearReferencingMethodUncertainty>
        </VectorLinearSpatialReferenceSystem>
      </planarReferencing>
